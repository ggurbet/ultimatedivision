    "dependencies": {
        "@metamask/onboarding": "1.0.1",
        "@velas/web3": "0.1.15",
        "buffer": "6.0.3",
        "casper-js-sdk": "2.14.0",
        "chart.js": "3.4.1",
        "copy-webpack-plugin": "11.0.0",
        "ethers": "5.5.1",
        "jest": "29.6.1",
        "jest-transform-stub": "2.0.0",
        "jsencrypt": "3.2.1",
        "react": "17.0.2",
        "react-chartjs-2": "3.0.3",
        "react-dom": "17.0.2",
        "react-redux": "7.2.9",
        "react-router-dom": "5.2.0",
        "react-scripts": "5.0.1",
